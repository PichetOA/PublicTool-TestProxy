$(document).ready(function() {
    var stickyNavTop = $('#navmelayang').offset().top; 
    var stickyNav = function(){
        var scrollTop = $(window).scrollTop();        
        if (scrollTop > stickyNavTop) { 
            $('#navmelayang').css({ 'position': 'fixed', 'top':0, 'z-index':9999 });
        } else {
            $('#navmelayang').css({ 'position': 'relative' });
        }
    };
    stickyNav();
    $(window).scroll(function() {
        stickyNav();
    });
});
